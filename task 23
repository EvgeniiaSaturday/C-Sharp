/*Задача 23: Напишите программу, которая принимает на вход число (N) и выдаёт 
таблицу кубов чисел от 1 до N.
3 -> 1, 8, 27.
5 -> 1, 8, 27, 64, 125*/

Console.Clear();

Console.Write("Insert number N: ");
int N=Convert.ToInt32(Console.ReadLine());
int num = 0;
int count = 1;

while (count < N)
{
  num=count*count*count;
  count++;
  Console.Write($"{num}, ");
}
N=N*N*N;
Console.Write($"{N}.");
